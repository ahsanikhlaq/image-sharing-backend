{
     "buildCommand": "npm run build",
     "outputDirectory": "dist",
     "functions": {
       "api/index.js": {
         "runtime": "nodejs18.x"
       }
     },
     "rewrites": [
       {
         "source": "/api/:path*",
         "destination": "/api/index.js"
       },
       {
         "source": "/share/:shareId",
         "destination": "/api/index.js"
       }
     ]
   }